{
  "name": "file-type",
  "version": "19.0.0",
  "description": "Detect the file type of a Buffer/Uint8Array/ArrayBuffer",
  "license": "MIT",
  "repository": "sindresorhus/file-type",
  "funding": "https://github.com/sindresorhus/file-type?sponsor=1",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "https://sindresorhus.com"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "node": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/browser.js",
      "import": "./dist/src/browser.js"
    },
    "./core": {
      "node": "./dist/src/core.js",
      "types": "./dist/src/core.d.ts"
    },
    "./browser": {
      "default": "./dist/src/browser.js",
      "import": "./dist/src/browser.js"
    },
    "./*": {
      "node": "./dist/src/*.js",
      "types": "./dist/src/*.d.ts",
      "default": "./dist/src/*.js",
      "import": "./dist/src/*.js"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "aegir build",
    "lint": "aegir lint",
    "lint:fix": "aegir lint --fix",
    "test": "npm run build && ava --fail-fast",
    "test:tsd": "tsd"
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    },
    "globals": {
      "Buffer": "readonly",
      "stream": "readonly"
    }
